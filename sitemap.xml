---
layout: null
---
<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
  {% for page in site.pages %}
    {% comment %} On exclut les pages techniques ou inutiles au SEO {% endcomment %}
    {% unless page.url contains '.xml' or page.url contains '.css' or page.sitemap == false or page.url contains '404' %}
    <url>
      <loc>{{ site.url }}{{ site.baseurl }}{{ page.url | replace: "index.html", "" }}</loc>
      <lastmod>{{ site.time | date_to_xmlschema }}</lastmod>
      <changefreq>monthly</changefreq>
      <priority>
        {% if page.url == "/" %}1.0
        {% elsif page.url contains "/services/" %}0.9
        {% elsif page.url contains "/detailing-auto-" %}0.8
        {% elsif page.url == "/contact/" %}0.8
        {% else %}0.6
        {% endif %}
      </priority>
    </url>
    {% endunless %}
  {% endfor %}
</urlset>
